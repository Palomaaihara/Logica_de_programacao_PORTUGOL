programa {
    funcao inicio() {
        inteiro numero, menor = 2147483648 // Inicializei com o menor valor possível para um inteiro
        inteiro contador

        para (contador = 1; contador <= 10; contador++) {
            escreva("Digite o ", contador, "º número: ")
            leia(numero)

            se (numero < menor) {
                menor = numero
            }
        }

        escreva("O maior número digitado foi: ", menor)
    }
}
