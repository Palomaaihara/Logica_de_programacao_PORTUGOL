programa {
  funcao inicio() {
    //Contagem regressiva de um numero inicio ate um numero i, onde o usurio possa escolher
    inteiro i, inicio, fim

    escreva("Digite em qual numero sua contagem regressiva deve comeÃ§ar? ")
    leia(inicio)
    escreva("Em qual numero sua contagem regressiva deve finalizar? ")
    leia(fim)

    para(i=inicio;i>=fim;i--){
      escreva("\n",i)
    }

  escreva("\nFIM")
 }
}
